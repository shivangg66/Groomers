{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "/company-details",
    "title": "company-details",
    "description": "Company Details",
    "type": "object",
    "properties": {
        "name": {
            "type": "string"
        },
        "location": {
            "type": "object",
            "properties": {
                "latitude": {
                    "type":"string",
                    "minimum": -90,
                    "maximum": 90
                },
                "longitude": {
                    "type":"string",
                    "minimum": -180,
                    "maximum": 180
                }
            }
        },
        "phone_number": {
            "type": "string",
            "maxLength": 10,
            "minLength": 10
        },
        "address": {
            "type": "string"
        },
        "email_id": {
            "type": "string",
            "format": "email"
        },
        "description": {
            "type": "string"
        }
    }
}